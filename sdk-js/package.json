{
  "name": "@diffuse/sdk-js",
  "version": "0.3.1",
  "private": true,
  "description": "Diffuse Prime SDK for JavaScript",
  "keywords": [
    "diffuse",
    "sdk",
    "javascript",
    "blockchain",
    "defi",
    "lending"
  ],
  "license": "Apache-2.0",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/diffuse-prime-sdk-js.mjs"
      }
    },
    "./package.json": "./package.json",
    "./abis/*": "./src/contracts/*/abi.json"
  },
  "main": "dist/diffuse-prime-sdk-js.mjs",
  "module": "dist/diffuse-prime-sdk-js.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src/contracts/*/abi.json",
    "LICENSE",
    "NOTICE"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test:unit": "vitest run --coverage",
    "test:unit:watch": "vitest"
  },
  "devDependencies": {
    "typescript": "5.8.2"
  },
  "peerDependencies": {
    "@diffuse/config": "0.1.1",
    "race-signal": "2.0.0",
    "viem": "2.38.5"
  }
}
