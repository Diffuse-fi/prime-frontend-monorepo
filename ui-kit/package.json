{
  "name": "@diffuse/ui-kit",
  "version": "0.1.0",
  "private": true,
  "description": "Diffuse Prime UI Kit React components",
  "keywords": [
    "diffuse",
    "ui-kit",
    "react",
    "components",
    "ui",
    "blockchain",
    "defi",
    "lending",
    "web3",
    "borrow"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/diffuse-prime-ui-kit.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/diffuse-prime-ui-kit.js"
      }
    },
    "./package.json": "./package.json"
  },
  "main": "dist/diffuse-prime-ui-kit.js",
  "module": "dist/diffuse-prime-ui-kit.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite"
  },
  "devDependencies": {
    "tailwindcss": "^4.1.12",
    "vite": "^7.1.2",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "peerDependencies": {
    "lucide-react": "^0.460.0",
    "radix-ui": "1.4.3",
    "react": ">=19",
    "react-dom": ">=19",
    "tailwind-merge": "3.3.1",
    "tailwind-variants": "2.1.0"
  }
}
