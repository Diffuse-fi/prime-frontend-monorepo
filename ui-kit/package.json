{
  "name": "@diffuse/ui-kit",
  "version": "0.1.0",
  "private": true,
  "description": "Diffuse Prime UI Kit React components",
  "keywords": [
    "diffuse",
    "ui-kit",
    "react",
    "components",
    "ui",
    "blockchain",
    "defi",
    "lending",
    "web3",
    "borrow"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/diffuse-prime-ui-kit.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/diffuse-prime-ui-kit.cjs"
      }
    },
    "./package.json": "./package.json",
    "./tw-preset": {
      "types": "./dist/tailwind.preset.d.ts",
      "import": "./dist/tailwind.preset.mjs",
      "require": "./dist/tailwind.preset.cjs"
    },
    "./safelist": {
      "types": "./dist/safelist.d.ts",
      "import": "./dist/safelist.mjs",
      "require": "./dist/safelist.cjs"
    }
  },
  "main": "dist/diffuse-prime-ui-kit.cjs",
  "module": "dist/diffuse-prime-ui-kit.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test:all": "npm run test:unit -- --run",
    "test:unit": "vitest --coverage"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.12",
    "vite": "^7.1.2",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "peerDependencies": {
    "lucide-react": "0.542.0",
    "radix-ui": "1.4.3",
    "react": ">=19",
    "react-dom": ">=19",
    "react-number-format": "5.4.4",
    "tailwind-merge": "3.3.1",
    "tailwind-variants": "2.1.0",
    "tailwindcss": "4.1.12"
  }
}
