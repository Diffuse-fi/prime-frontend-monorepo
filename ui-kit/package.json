{
  "name": "@diffuse/ui-kit",
  "version": "0.1.0",
  "private": true,
  "description": "Diffuse Prime UI Kit React components",
  "keywords": [
    "diffuse",
    "ui-kit",
    "react",
    "components",
    "ui",
    "blockchain",
    "defi",
    "lending",
    "web3",
    "borrow"
  ],
  "license": "Apache-2.0",
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/diffuse-prime-ui-kit.mjs"
      }
    },
    "./package.json": "./package.json",
    "./preset.css": "./dist/preset.css",
    "./*": {
      "import": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.mjs"
      }
    }
  },
  "main": "dist/diffuse-prime-ui-kit.mjs",
  "module": "dist/diffuse-prime-ui-kit.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "NOTICE"
  ],
  "scripts": {
    "build": "vite build && npm run ladle:build",
    "dev": "concurrently \"cross-env BUILD_WATCH=1 vite build --watch\" \"ladle serve\" --kill-others --success last",
    "ladle": "ladle serve",
    "ladle:build": "ladle build",
    "ladle:preview": "ladle preview",
    "test:snap:update": "vitest run --update snap.test.tsx",
    "test:snap:update:component": "vitest run --update --",
    "test:unit": "vitest run --coverage --exclude '**/*.snap.test.tsx'",
    "test:unit:snap": "vitest run snap.test.tsx",
    "test:unit:watch": "vitest"
  },
  "devDependencies": {
    "@ladle/react": "^5.1.1",
    "@tailwindcss/cli": "4.1.12",
    "@tailwindcss/vite": "4.1.12",
    "fast-glob": "^3.3.3",
    "jsdom-testing-mocks": "^1.15.2",
    "typescript": "5.8.2"
  },
  "peerDependencies": {
    "@tanstack/react-table": "8.21.3",
    "clsx": "2.1.1",
    "copy-to-clipboard": "3.3.3",
    "lucide-react": "0.542.0",
    "radix-ui": "1.4.3",
    "react": ">=19.1.0",
    "react-dom": ">=19.1.0",
    "react-number-format": "5.4.4",
    "tailwind-merge": "3.3.1",
    "tailwind-variants": "2.1.0",
    "tailwindcss": "4.1.12"
  }
}
